{
  "name": "budget-tracker",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A personal finance application for tracking expenses, income, and budget management",
  "author": "Stijnus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Stijnus/Budget-Tracker.git"
  },
  "keywords": [
    "budget",
    "finance",
    "expense-tracker",
    "react",
    "typescript",
    "supabase"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "types:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/lib/database.types.ts",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "pre-commit": "./scripts/check-typescript.sh",
    "start": "vite preview",
    "clean": "rm -rf node_modules dist .turbo",
    "update-deps": "npm update",
    "test": "echo \"No tests specified\" && exit 0",
    "translations:validate": "node scripts/validate-translations.mjs",
    "translations:validate:fix": "node scripts/validate-translations.mjs --fix",
    "translations:audit": "node scripts/audit-translations.mjs",
    "translations:audit:fix": "node scripts/audit-translations.mjs --fix",
    "translations:restructure": "node scripts/restructure-translations.mjs"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "tsc-files --noEmit"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.10",
    "@radix-ui/react-avatar": "^1.1.6",
    "@radix-ui/react-checkbox": "^1.2.2",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.10",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.3.3",
    "@radix-ui/react-scroll-area": "^1.2.5",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.8",
    "@radix-ui/react-toast": "^1.2.10",
    "@radix-ui/react-tooltip": "^1.2.3",
    "@supabase/supabase-js": "^2.44.4",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.5.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.0",
    "react-router-dom": "^7.5.1",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@babel/generator": "^7.24.0",
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@eslint/js": "^9.25.1",
    "@shadcn/ui": "^0.0.4",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.25.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^10.4.5",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "lucide-react": "^0.503.0",
    "postcss": "^8.4.35",
    "supabase": "^1.226.4",
    "tailwindcss": "^3.4.17",
    "tsc-files": "^1.1.4",
    "tsx": "^4.19.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^5.4.18"
  }
}
